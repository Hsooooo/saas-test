<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.illunex.emsaasrestapi.partnership.mapper.PartnershipAdditionalMapper">

    <insert id="insertByPartnershipAdditionalVO" parameterType="PartnershipAdditionalVO">
        INSERT INTO partnership_additional (
            partnership_idx,
            attr_key,
            attr_value,
            create_date,
            update_date
        )
        VALUES (
           #{partnershipIdx},
           #{attrKey},
           #{attrValue},
           NOW(),
           NOW()
       )
    </insert>

    <delete id="deleteByPartnershipIdx">
        DELETE FROM partnership_additional
        WHERE partnership_idx = #{partnershipIdx}
    </delete>

</mapper>